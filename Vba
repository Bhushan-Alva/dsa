import win32com.client
import pyautogui
import time

file_path = r"C:\Users\YourName\Documents\test.xlsx"

excel = win32com.client.Dispatch("Excel.Application")
excel.Visible = True

wb = excel.Workbooks.Open(file_path)

time.sleep(5)  # Wait for Excel UI to load

# Example: Click label dropdown (coordinates must be adjusted)
pyautogui.click(300, 150)

time.sleep(1)

# Click specific label option
pyautogui.click(300, 200)

time.sleep(1)

wb.Save()
wb.Close()
excel.Quit()
